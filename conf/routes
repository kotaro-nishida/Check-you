# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
#GET     /                           	  controllers.Application.index()
GET     /                           	  controllers.ChecksController.index()
GET     /result                           controllers.ChecksController.result()
GET     /recent/:page                           controllers.ChecksController.recent(page : Integer)
GET     /recent                           controllers.ChecksController.recent(page : Integer = 1)
GET     /resultId/:id                         controllers.ChecksController.resultId(id : Long)
GET     /resultId                         controllers.ChecksController.resultId(id : Long = 1)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
